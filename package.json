        "grunt-ts": "^6.0.0-beta.21",
        "grunt-tslint": "^5.0.2",
        "rollup-plugin-node-resolve": "^4.0.0",
        "rollup-plugin-replace": "^2.1.0",
        "rollup-plugin-uglify": "^6.0.0",
        "selenium-webdriver": "^4.0.0-alpha.1",
        "tslint": "^5.11.0",
        "tslint-microsoft-contrib": "^6.0.0",
        "typescript": "2.5.3"
